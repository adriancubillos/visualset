{
  "name": "visualset",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "dev:with-tests": "npm run enable-test-types && next dev",
    "build": "npm run && prisma generate && next build",
    "build:production": "npm run disable-test-types && prisma generate && npx prisma migrate deploy && next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:typecheck": "vitest typecheck",
    "enable-test-types": "node -e \"const fs=require('fs');const cfg=JSON.parse(fs.readFileSync('tsconfig.json','utf8'));cfg.exclude=cfg.exclude.filter(e=>e!=='test/**/*');fs.writeFileSync('tsconfig.json',JSON.stringify(cfg,null,2))\"",
    "disable-test-types": "node -e \"const fs=require('fs');const cfg=JSON.parse(fs.readFileSync('tsconfig.json','utf8'));if(!cfg.exclude.includes('test/**/*'))cfg.exclude.push('test/**/*');fs.writeFileSync('tsconfig.json',JSON.stringify(cfg,null,2))\"",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.16.1",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-table": "^8.21.3",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^19",
    "@vercel/blob": "^2.0.0",
    "date-fns": "^4.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "next": "15.5.3",
    "prisma": "^6.16.1",
    "react": "19.1.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "tailwindcss": "^4.1.13",
    "typescript": "^5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.4",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^27.0.0",
    "vitest": "^3.2.4"
  }
}
